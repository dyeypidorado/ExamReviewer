<h1>My Exams</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Date Created</th>
    <th>Date Updated</th>
  </tr>
<% @exam.each do |exam|%>  
  <tr>
    <td><%= exam.name %> | </td>
    <td><%= exam.created_at %> | </td>
    <td><%= exam.updated_at %> | </td>
    <td><%= link_to 'View Questions', admin_exam_questions_path(exam.id) %> | </td>
    <td><%= link_to 'Delete', admin_exam_path(exam), :method => :delete %></td>
  <tr>
<% end %>
</table>
<br/><hr>
<%= link_to 'Create New Exam', new_admin_exam_path %> | 
<%= link_to 'Back to Home', admin_root_path %>
